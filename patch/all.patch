-++-++    chunk_size = s_rate / 2000; 
-++-++    chunks = (float *)calloc(NUM_CHUNKS, sizeof(float));
-++-++    peak = 0.0f;
-++-++    atten = 1.0f;
-++-++    atten_lp = 1.0f;
-++-++    delta = 0.0f;
