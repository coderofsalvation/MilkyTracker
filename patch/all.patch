---++                                      (buffer_len - 1)] *
---++                                   atten_lp);
---++      buffer_write(inbuffer,posR, max,buffer[(buffer_pos * 2 - delay * 2 + 1) &
---++                                      (buffer_len - 1)] *
---++                                   atten_lp);
---++      buffer_pos++;
---++
---++    }
