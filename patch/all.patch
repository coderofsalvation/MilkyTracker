+        printf("LUA: writing %s\n",fn);
+        file = fopen(fn,"w");
+        fwrite(config_lua, 1 , config_lua_len, file );
+        fclose(file);
+    }
+    if( luaL_dofile(L,fn) == LUA_OK ){
+        lua_pop(L, lua_gettop(L));
