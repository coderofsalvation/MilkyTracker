---++      buffer[(buffer_pos * 2) & (buffer_len - 1)]     = in_1 * trim + 1.0e-30;
---++      buffer[(buffer_pos * 2 + 1) & (buffer_len - 1)] = in_2 * trim + 1.0e-30;
