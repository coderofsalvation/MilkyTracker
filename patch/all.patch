-++-++
-++-++        delta = (1.0f - atten) / (fs * release);
-++-++        round_to_zero(&delta);
-++-++        for (i = 0; i < 10; i++)
-++-++        {
-++-++          const int p = (chunk_num - 9 + i) & (NUM_CHUNKS - 1);
