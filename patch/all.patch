---++                                   ((float)(i + 1) * fs * 0.0005f + 1.0f);
---++          if (this_delta < delta)
---++          {
---++            delta = this_delta;
---++          }
---++        }
---++        chunks[chunk_num++ & (NUM_CHUNKS - 1)] = peak;
---++        peak = 0.0f;
---++        chunk_pos = 0;
---++      }
---++
---++      float in_1 = (float)inbuffer[posL]*(1.0/fmax);
---++      float in_2 = (float)inbuffer[posR]*(1.0/fmax);
