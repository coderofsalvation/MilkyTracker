++++-++#define NUM_CHUNKS 16
++++-++#define BUFFER_TIME 0.0053
++++-++#define DB_CO(g) ((g) > -90.0f ? powf(10.0f, (g) * 0.05f) : 0.0f)
++++-++#define CO_DB(v) (20.0f * log10f(v))
