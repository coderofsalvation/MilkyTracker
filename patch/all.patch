++--++	Limiter() : 
++--++    fs(-1),
++--++    fmax(32678.0f),
++--++    limit(-0.05),
++--++    release(0.01),
++--++    buffer(NULL),
++--++    ingain(1.0),
++--++    attenuation(0.0)
